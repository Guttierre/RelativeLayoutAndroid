<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_main" tools:context=".MainActivity"
    android:background="@drawable/bk6">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Acesse sua conta"
        android:id="@+id/txtConta"
        android:textSize="17sp"
        android:textStyle="bold"
        android:textColor="#FFFFFF"
        android:gravity="center"
        android:drawableTop="@drawable/ic_conta"
        android:clickable="true"
        android:onClick=""
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:layout_alignTop="@+id/txtCalendario"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calendario"
        android:id="@+id/txtCalendario"
        android:textSize="17sp"
        android:gravity="center"
        android:drawableTop="@drawable/ic_calendario"
        android:layout_marginRight="43dp"
        android:layout_marginEnd="43dp"
        android:layout_marginTop="8dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textStyle="bold"
        android:textColor="#FFFFFF"
        android:clickable="true"
        android:onClick=""/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Atendimento"
        android:textSize="17sp"
        android:gravity="center"
        android:drawableTop="@drawable/ic2"
        android:layout_marginTop="47dp"
        android:id="@+id/txtAtendimento"
        android:layout_marginRight="14dp"
        android:layout_marginEnd="14dp"
        android:textStyle="bold"
        android:textColor="#FFFFFF"
        android:clickable="true"
        android:onClick=""
        android:layout_below="@+id/txtCalendario"
        android:layout_alignRight="@+id/txtConta"
        android:layout_alignEnd="@+id/txtConta" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Telefones"
        android:textSize="17sp"
        android:gravity="center"
        android:drawableTop="@drawable/ic_telefone"
        android:id="@+id/txtTelefone"
        android:textStyle="bold"
        android:textColor="#FFFFFF"
        android:clickable="true"
        android:onClick=""
        android:layout_marginTop="38dp"
        android:layout_below="@+id/txtAtendimento"
        android:layout_alignLeft="@+id/txtAtendimento"
        android:layout_alignStart="@+id/txtAtendimento" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Simulador"
        android:textSize="17sp"
        android:gravity="center"
        android:drawableTop="@drawable/ic_simulador"
        android:id="@+id/txtSimulador"
        android:textStyle="bold"
        android:textColor="#FFFFFF"
        android:clickable="true"
        android:onClick=""
        android:layout_alignBottom="@+id/txtAtendimento"
        android:layout_alignLeft="@+id/txtCalendario"
        android:layout_alignStart="@+id/txtCalendario" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Loterias Banco"
        android:gravity="center"
        android:drawableTop="@drawable/ic_loterias"
        android:textStyle="bold"
        android:textColor="#FFFFFF"
        android:id="@+id/textView5"
        android:clickable="true"
        android:onClick=""
        android:layout_alignTop="@+id/txtTelefone"
        android:layout_alignLeft="@+id/txtCalendario"
        android:layout_alignStart="@+id/txtCalendario" />

</RelativeLayout>
